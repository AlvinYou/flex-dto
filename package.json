{
  "name": "flex-dto",
  "version": "0.1.1",
  "description": "Flexible DTO mapper for frontend - snake_case/camelCase conversion with class instantiation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "tsx tests/index.test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dto",
    "api",
    "snake_case",
    "camelCase",
    "mapping",
    "transform",
    "frontend",
    "typescript",
    "class",
    "response"
  ],
  "author": "Alvin You <https://github.com/AlvinYou>",
  "license": "MIT",
  "homepage": "https://github.com/AlvinYou/flex-dto#readme",
  "peerDependencies": {
    "@types/node": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": ">=18.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": ">=5.0.0"
  }
}
